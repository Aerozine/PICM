{
    "dx": 0.01,
    "dy": 0.01,
    "dt": 0.001,
    "nx": 10,
    "ny": 11,
    "nt": 10,
    "density": 1000,
    "sampling_rate": 5,

    "write_u":             true,
    "write_v":             true,
    "write_p":             true,
    "write_div":           true,
    "write_norm_velocity": true,

    "folder":   "results",
    "filename": "simulation",

    "velocityu": {
        "rectangle": {
            "val": 1.0,
            "x1": "nx/2-1",
            "y1": "ny/2-1",
            "x2": "nx/2+1",
            "y2": "ny/2+1"
        }
    },
"solver": {  
    "type": "red_black_gauss_seidel",
    "max_iterations": 5000,
    "tolerance": 1e-1
}
}
